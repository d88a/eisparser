# EIS Parser Configuration
# Скопируйте этот файл как .env и заполните значения

# Gemini AI API Key
GEMINI_API_KEY=your_api_key_here

# Database
DATABASE_PATH=../results/eis_data.db

# Scraper settings
STAGE4_HEADLESS=true
STAGE4_USE_REAL_CHROME=true
STAGE4_PAGE_TIMEOUT_S=60

